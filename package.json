{
  "name": "tocgen",
  "version": "0.0.3",
  "description": "A simple opinionated Table of Contents generator",
  "main": "lib/index.js",
  "scripts": {
    "prepublish": "in-publish && npm run build || in-install",
    "build": "babel src --out-dir lib --copy-files",
    "watch": "babel src --out-dir lib --copy-files --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/boopathi/tocgen"
  },
  "bin": {
    "tocgen": "lib/tocgen.js"
  },
  "keywords": [
    "tocgen",
    "table",
    "of",
    "contents"
  ],
  "author": "boopathi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/boopathi/tocgen/issues"
  },
  "homepage": "https://github.com/boopathi/tocgen",
  "dependencies": {
    "anchor-markdown-header": "^0.5.3",
    "bluebird": "^3.4.7",
    "colors": "^1.1.2",
    "commander": "^2.8.1"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "in-publish": "^2.0.0"
  }
}
